(function () {
    let selectArr = [];
    let dataList = [];
    let areaCode = '49' //德国
    let params = {
        countryCode: 'DE',
        zip: ''
    };

    if (marketId == 30) { //法国
        params.countryCode = 'FR';
        areaCode = '33';
    } else if (currencyId == 5) { //英国
        params.countryCode = 'GB';
        areaCode = '44';
    } else if (marketId == 46) { //意大利
        params.countryCode = 'IT';
        areaCode = '39';
    };
    window.country_code = params.countryCode;

    //输入邮编请求城市
    $('body').on('keyup input', '[name="zip_code"]', debounce(function () {
        $('#submitForm').attr('disabled', false).removeClass('stop-order');
        params.zip = $(this).val().trim();
        if (params.zip) {
            //法国
            if (marketId == 30) {
                if (params.zip.length >= 3) {
                    getESdata(params).then(res => {
                        let list = '';
                        if (res.length == 0) {
                            $('.option-list').removeClass('show');
                            $('[name="city"], [name="state"]').val('').attr({ 'readonly': false, 'maxlength': 200 });
                            return;
                        }
                        dataList = JSON.parse(JSON.stringify(res));
                        dataList.forEach(item => {
                            list += `<li data-id="${item.id}">${item.zip_code}, ${item.city}, ${item.state}</li>`;
                        });
                        $('.option-list').addClass('show').find('.select-option').html(list);
                    });
                } else {

                }
            } else {
                //德国、英国、意大利
                if ((marketId == 44 && params.zip.length == 5) || (currencyId == 5 && params.zip.length >= 7) || (marketId == 46 && params.zip.length == 5)) {
                    //德国、意大利不可派送邮编校验
                    if (marketId === '44' || marketId === '46') {
                        let notDeliveryArr = [];
                        if (marketId === '44') {
                            notDeliveryArr = ['78266', '27498'];
                        } else {
                            notDeliveryArr = notDeliveryZipList.map(v => v.zip);
                        }
                        if (typeof checkNotDeliveryZip !== 'undefined') {
                            checkNotDeliveryZip(params.zip, notDeliveryArr);
                        }
                    }
                    
                    getESdata(params).then(res => {
                        let list = '';
                        if (res.city) {
                            selectArr = res.city.toString().split(',');
                            if (marketId == 46) {
                                $('[name="state"]').trigger('focus').val(res.state[0]).trigger('blur').trigger('change');
                            } 

                            $('[name="state"], [name="city"]').attr({ 'readonly': true, 'maxlength': 0 });
                            $('[name="city"]').parent('.form-group').addClass('auto-value form-group-label').trigger('blur').trigger('change');
                            if (selectArr.length == 1) {
                                $('.select-city').removeClass('show').html('');
                                $('[name="city"]').val(selectArr[0]).trigger('blur').trigger('change');
                            } else {
                                selectArr.forEach(item => {
                                    list += `<li data-value="${item}">${item}</li>`
                                });
                                $('[name="city"]').val('');
                                $('.select-city').addClass('show').html(list);
                            }
                           
                        } else {
                            $('[name="city"]').parent('.form-group').removeClass('auto-value');
                            $('[name="city"]').val('').attr({ 'readonly': false, 'maxlength': 20 });
                            $('[name="city"], [name="state"]').val('');
                            $('.select-city').removeClass('show').html('');
                        }
                    })
                } else {
                    $('[name="city"], [name="state"]').val('');
                    $('.select-city').removeClass('show').html('')
                };
            }
        }

    }, 500));

    

    $('#data-list').on('click', '.select-option li', function () {
        let rowId = $(this).attr('data-id');
        let dataRow = dataList.filter(v => v.id == rowId);
        $('[name="zip_code"]').val(dataRow[0].zip_code).trigger('focus').trigger('change');
        $('[name="city"]').val(dataRow[0].city).trigger('focus').trigger('change');
        $('[name="state"]').val(dataRow[0].state).trigger('focus').trigger('blur').trigger('change');
        if (marketId == 30) {
            $('[name="city"], [name="state"]').attr({ 'readonly': true, 'maxlength': 0 });
        }

        $(this).parents('.option-list').removeClass('show');
    });


    //当城市为多个时 再点击city输入框 显示下拉列表
    $('body').on('click', '.auto-value [name="city"]', function () {
        if (selectArr.length > 1) {
            $('.select-city').addClass('show');
        } else {
            $('.select-city').removeClass('show');
        }
    })

    $('body').on('click', '.select-city li', function () {
        let eleVal = $(this).data('value');
        $('[name="city"]').focus().val(eleVal).trigger('blur').trigger('change');
        $(this).parent().removeClass('show');
    })

    $('body').on('input keyup', '[name="phone2"]', function (e) {
        if (e.keyCode == 32) return;
        let phone = String($(this).val());
        if (!phone) {
            $('input[name="phone"]').val('');
            return;
        }
        phone = phone.trim().replace(/\+|\s/, '');
        var fullPhone = phone.substring(0, areaCode.length) != areaCode ? areaCode + phone : phone;
        $('input[name="phone"]').val(fullPhone).trigger('blur').trigger('change');
        $('[name="phone2"]').val(phone);
    });

    $('body').on('input keyup', 'input[name="street"], input[name="address1"]', function () {
        var street = $('[name="street"]').val();
        var address = $('[name="address1"]').val();
        $('input[name="address"]').val(street.trim() === address.trim() ? street : street + ' ' + address);

        if (street.toLowerCase().includes('packstation') || street.toLowerCase().includes('poatfiliale')) {
            if ($('.black-address').length == 0) {
                $('[name="street"]').after('<small class="red black-address">Packstation&Poatfiliale keine Lieferung</small>');
            }
        } else {
            $('.black-address').remove();
        }
    });

    // 無街道號
    $('#no-number').remove();
})();

